{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "src",
    "downlevelIteration": true
  },
  "include": ["src"]
}
